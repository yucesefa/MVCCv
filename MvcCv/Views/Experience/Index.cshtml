@using MvcCv.Models.Entity
@model List<tblExpreience>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Experience Lists</h2>
<table class="table table-hover">
     <tr>
         <th>#</th>
         <th>Title</th>
         <th>Subtitle</th>
         <th>Date</th>
         <th>Delete</th>
         <th>Update</th>
     </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Title</td>
            <td>@item.Subtitle</td>
            <td>@item.Date</td>
            <td><a href="/Experience/DeleteExperience/@item.ID" class="btn btn-danger">Delete</a></td>
            <td><a href="/Experience/GetExperience/@item.ID" class="btn btn-success">Update</a></td>
            <td></td>
        </tr>
    }
</table>
<a href="/Experience/AddExperience/" class="btn btn-info"> Add New Experience</a>

