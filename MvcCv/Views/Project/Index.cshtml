@using MvcCv.Models.Entity
@model List<tblProject>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Experience Lists</h2>
<table class="table table-hover">
    <tr>
        <th>#</th>
        <th>Title</th>
        <th>Link</th>
        <th>Image</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Title</td>
            <td>@item.Link</td>
            <td><img src="@item.ImageUrl"style="width:100px; height:100px;" /></td>
            <td><a href="/Project/DeleteProject/@item.ID" class="btn btn-danger">Delete</a></td>
            <td><a href="/Project/EditProject/@item.ID" class="btn btn-success">Update</a></td>
            <td></td>
        </tr>
    }
</table>
<a href="/Project/AddProject/" class="btn btn-info"> Add New Project</a>

