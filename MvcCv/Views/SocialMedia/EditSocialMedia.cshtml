@model MvcCv.Models.Entity.tblSocialMedia
@{
    ViewBag.Title = "EditSocialMedia";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<form method="post">
    @Html.HiddenFor(x => x.SocialMediaID)
    @Html.Label("Social Media:")
    @Html.TextBoxFor(x => x.SocialMedia, new { @class = "form-control" })
    <br />
    @Html.Label("Link:")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("ImageUrl:")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Edit</button>
</form>